{{text-field
    value=value
    type=type
    pattern=pattern
    maxlength=maxlength
    class="second-factor-token-input"
    id=inputId
    autocapitalize="off"
    autocomplete="one-time-code"
    autocorrect="off"
    autofocus="autofocus"
    placeholder=placeholder
    input=(action "onInput")
}}
