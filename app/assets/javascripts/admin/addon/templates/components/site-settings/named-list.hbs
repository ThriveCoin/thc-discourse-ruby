{{list-setting
  value=settingValue
  settingName=setting.setting
  choices=settingChoices
  nameProperty="name"
  valueProperty="value"
  onChange=(action "onChangeListSetting")
  options=(hash
    allowAny=allowAny
  )
}}

{{setting-validation-message message=validationMessage}}
<div class="desc">{{html-safe setting.description}}</div>
