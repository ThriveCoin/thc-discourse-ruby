  "globals": {
    "_": "off",
    "acceptance": "off",
    "asyncRender": "off",
    "bootbox": "off",
    "click": "off",
    "count": "off",
    "currentPath": "off",
    "currentRouteName": "off",
    "currentURL": "off",
    "currentUser": "off",
